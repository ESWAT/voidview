{
  "name": "VoidView",
  "version": "1.0.0",
  "description": "Stare into the endless void of your image collection.",
  "main": "main.js",
  "scripts": {
    "start": "electron-webpack dev",
    "compile": "electron-webpack",
    "lint": "eslint ./src",
    "dist-mac": "npm run compile && node scripts.js dist mac",
    "dist-linux": "npm run compile && node scripts.js dist linux",
    "dist-win": "npm run compile && node scripts.js dist win",
    "push-mac": "node scripts.js push mac",
    "push-linux": "node scripts.js push mac",
    "push-win": "node scripts.js push mac"
  },
  "repository": "https://gitlab.com/eswat/yuffie",
  "author": "Philip Karpiak <philip@eswat.ca>",
  "devDependencies": {
    "css-loader": "^2.1.0",
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5",
    "electron-webpack": "^2.6.1",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "webpack": "^4.29.0"
  },
  "electronWebpack": {
    "title": "VoidView",
    "main": {
      "webpackConfig": "webpack.main.js"
    },
    "renderer": {
      "webpackConfig": "webpack.renderer.js"
    }
  },
  "build": {
    "appId": "voidview",
    "mac": {
      "darkModeSupport": true
    }
  },
  "dependencies": {
    "clusterize.js": "^0.18.1",
    "electron-window-state": "^5.0.3",
    "image-type": "^3.0.0",
    "read-chunk": "^3.0.0",
    "shuffle-array": "^1.0.1",
    "source-map-support": "^0.5.10"
  }
}
